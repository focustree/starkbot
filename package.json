{
  "name": "starkbot",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18",
    "pnpm": "8"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "nx start discord-bot",
    "infra": "nx run-many --target start --projects stack,kube --parallel --output-style stream",
    "infra:deploy": "nx start stack",
    "infra:deploy:all": "nx deploy-all stack",
    "infra:deploy:website": "nx update-website stack",
    "infra:destroy": "nx delete stack",
    "infra:destroy:all": "nx destroy-all stack",
    "kube:deploy": "nx start kube",
    "kube:clean": "nx delete kube",
    "web": "nx start website",
    "docker:login": "nx login-ecr discord-bot",
    "docker:deploy:base": "nx docker-deploy-base discord-bot",
    "docker:deploy:app": "nx deploy discord-bot"
  },
  "dependencies": {
