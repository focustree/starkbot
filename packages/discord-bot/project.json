{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/discord-bot/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/discord-bot",
        "main": "packages/discord-bot/src/index.ts",
        "tsConfig": "packages/discord-bot/tsconfig.lib.json",
        "assets": ["packages/discord-bot/*.md"]
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node dist/packages/discord-bot/src/index.js"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/discord-bot/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/discord-bot"],
      "options": {
        "jestConfig": "packages/discord-bot/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
